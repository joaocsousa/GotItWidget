        <attr name="oneTimeHintView_backgroundColor" format="color"/>
        <attr name="oneTimeHintView_cardColor" format="color"/>
        <attr name="oneTimeHintView_title" format="string"/>
        <attr name="oneTimeHintView_description" format="string"/>
